{
  "project": {
    "version": "1.0.0-beta.24",
    "name": "public"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "bc",
      "mobile": false,
      "styles": [
        "common/javascripts/vendor/bootstrap/css/bootstrap.min.css",
        "common/styles//clean-blog.min.css",
        "common/javascripts/vendor/font-awesome/css/font-awesome.min.css",
        "styles.css"
      ],
      "scripts": [
        "common/javascripts/vendor/jquery/jquery.min.js",
        "common/javascripts/vendor/bootstrap/js/bootstrap.min.js",
        "common/javascripts/clean-blog.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
